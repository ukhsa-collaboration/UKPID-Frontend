{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["wdio"],
  "parser": "vue-eslint-parser",
  "extends": [
    "plugin:vue/vue3-recommended",
    "prettier",
    "plugin:wdio/recommended"
  ],
  "overrides": [
    {
      "files": ["src/renderer/pages/*.vue"],
      "rules": {
        "vue/multi-word-component-names": "off"
      }
    }
  ]
}
